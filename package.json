    "glob": "^10.4.1",
    "json5": "^2.2.3",
    "npm-check": "^6.0.1",
    "prompts": "^2.4.2",
    "recursive-readdir": "^2.2.3",
    "yaml": "^2.4.3"
  },
  "devDependencies": {
